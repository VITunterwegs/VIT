{
    "Type":"sap.ui.core.mvc.JSONView",
    "controllerName":"vit.Navigation",
    "content":[
        {
            "Type":"sap.m.Page",
            "id":"navigation",
            "showNavButton":false,
            "backgroundDesign":"Transparent",
            "customHeader":[
                {
                    "Type":"sap.m.Bar",
                    "contentLeft":[
                        {
                            "Type":"sap.m.Image",
                            "src":"img/Logo_36x36_120dpi.png"
                        }
                    ],
                    "contentMiddle":[
                        {
                            "Type":"sap.m.Label",
                            "text":"Men√º"
                        }
                    ],
                    "contentRight":[
                        {
                            "Type":"sap.m.Button",
                            "icon":"sap-icon://arrow-right",
                            "press":"goBack"
                        }
                    ]
                }
            ],
            "content":[
                {
                    "Type":"sap.m.List",
                    "type":"sap.m.ListType.Navigation",
                    "items":[
                        {
                            "Type":"sap.m.StandardListItem",
                            "title":"Startseite",
                            "icon":"sap-icon://home",
                            "type":"Navigation",
                            "press":"handleHomePress"
                        },
                        {
                            "Type":"sap.m.StandardListItem",
                            "title":"Suche",
                            "icon":"sap-icon://search",
                            "type":"Navigation",
                            "press":"handleSearchPress"
                        },
                        {
                            "Type":"sap.m.StandardListItem",
                            "title":"Melden",
                            "icon":"sap-icon://action",
                            "type":"Navigation",
                            "press":"handleNotifyPress"
                        },
                        {
                            "Type":"sap.m.StandardListItem",
                            "title":"Favoriten",
                            "icon":"sap-icon://favorite",
                            "type":"Navigation",
                            "press":"handleFavoritesPress"
                        },
                        {
                            "Type":"sap.m.StandardListItem",
                            "title":"Einstellungen",
                            "icon":"sap-icon://action-settings",
                            "type":"Navigation",
                            "press":"handleSettingsPress"
                        },
                        {
                            "Type":"sap.m.StandardListItem",
                            "title":"Hilfe",
                            "icon":"sap-icon://sys-help",
                            "type":"Navigation",
                            "press":"handleHelpPress"
                        },
                        {
                            "Type":"sap.m.StandardListItem",
                            "title":"Impressum",
                            "icon":"sap-icon://message-information",
                            "type":"Navigation",
                            "press":"handleImpressumPress"
                        }
                    ]
                }
            ]
        }
    ]
}