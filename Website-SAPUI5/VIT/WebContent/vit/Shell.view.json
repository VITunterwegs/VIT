{
	"Type":"sap.ui.core.mvc.JSONView",
	"controllerName":"vit.Shell",
	"content":[
		{
			"Type":"sap.ui.unified.Shell",
			"id":"myShell",
			"icon":"img/Logo_96x96_320dpi.png",
			"headItems":[
				{
					"Type":"sap.ui.unified.ShellHeaderItem",
					"tooltip":"Menu",
					"icon":"sap-icon://menu2",
					"press":"handlePressConfiguration",
					"id":"menuItem"
				}
				
			],
			"headEndItems":[
				{
					"Type":"sap.ui.unified.ShellHeaderItem",
					"tooltip":"Home",
					"icon":"sap-icon://home",
					"press":"handleHomePress"
				}
				
			],
			"content":[
				{
					"Type":"sap.m.App",
					"id":"myApp",
					"backgroundImage":"img/startscreen_header.png",
					"pages":[
						{
							"Type":"sap.ui.view",
							"id":"homePage",
							"viewName":"vit.Home",
							"type":"JSON"
						},
						{
							"Type":"sap.ui.view",
							"id":"searchPage",
							"viewName":"vit.Search",
							"type":"JSON"
						},
						{
							"Type":"sap.ui.view",
							"id":"favPage",
							"viewName":"vit.Favorites",
							"type":"JSON"
						},
						{
							"Type":"sap.ui.view",
							"id":"notifyPage",
							"viewName":"vit.Notify",
							"type":"JSON"
						}
						
					]
					
				}
				
			],
			"paneContent":[
				{
					"Type":"sap.m.List",
					"items":[
						{
							"Type":"sap.m.ActionListItem",
							"text":"Home",
							"press":"handleHomePress"
						},
						{
							"Type":"sap.m.ActionListItem",
							"text":"Suche",
							"press":"handleSearchPress"
						},
						{
							"Type":"sap.m.ActionListItem",
							"text":"Melden",
							"press":"handleNotifyPress"
						},
						{
							"Type":"sap.m.ActionListItem",
							"text":"Favoriten",
							"press":"handleFavoritesPress"
						},
						{
							"Type":"sap.m.ActionListItem",
							"text":"Einstellungen",
							"press":"handleSearchPress"
						},
						{
							"Type":"sap.m.ActionListItem",
							"text":"Hilfe",
							"press":"handleNotifyPress"
						},
						{
							"Type":"sap.m.ActionListItem",
							"text":"Impressum",
							"press":"handleFavoritesPress"
						}
						
					]
					
				}
				
			]
			
		}
		
	]
}