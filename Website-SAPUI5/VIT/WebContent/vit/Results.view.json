{
    "Type":"sap.ui.core.mvc.JSONView",
    "controllerName":"vit.Result",
    "content":[
        {
            "Type":"sap.m.Page",
            "id":"result",
            "backgroundDesign":"Transparent",
            "content":[
                {
                    "Type":"sap.m.FlexBox",
                    "id":"flexbox",
                    "height":"43%",
                    "alignItems":"Center",
                    "justifyContent":"Center",
                    "direction":"Coloumn",
                    "items":[
                        {
                        	"Type":"sap.m.Slider",
                        	"width":"180px",
                    		"change":"handleSliderChange"
                        },
                        {
                            "Type":"sap.m.Button",
                            "text":"Liste",
                            "icon":"sap-icon://search",
                            "iconFirst":true,
                            "width":"180px",
                            "press":"handleListPress"
                        },
                        {
                            "Type":"sap.m.Button",
                            "text":"Karte",
                            "icon":"sap-icon://action",
                            "iconFirst":true,
                            "width":"180px",
                            "press":"handleMapPress"
                        }
                    ]
                },
                {
                    "Type":"sap.m.Table",
                    "columns":[
                        {
                            "Type":"sap.m.Column",
                            "header":{
                                "Type":"sap.m.Label",
                                "text":"Linie"
                            }
                        },
                        {
                            "Type":"sap.m.Column",
                            "header":{
                                "Type":"sap.m.Label",
                                "text":"Richtung"
                            }
                        },
                        {
                            "Type":"sap.m.Column",
                            "header":{
                                "Type":"sap.m.Label",
                                "text":"Versp√§tung"
                            }
                        },
                        {
                            "Type":"sap.m.Column",
                            "header":{
                                "Type":"sap.m.Label",
                                "text":"Ankunft"
                            }
                        }
                    ]
                }
            ],
            "customHeader":[
                {
                    "Type":"sap.m.Bar",
                    "contentLeft":[
                        {
                            "Type":"sap.m.Button",
                            "icon":"sap-icon://menu2",
                            "press":"showMenu"
                        }
                        
                    ],
                    "contentMiddle":[
                        {
                            "Type":"sap.m.Label",
                            "text":"Einstellung"
                        }
                        
                    ]
                    
                }
                
            ]
            
        }
        
    ]
}